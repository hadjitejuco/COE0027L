//simple if
public class Sample12 {
    public static void main(String[] args) {
        int n;
        n = 99;
        
        if (n<100){
            System.out.println("The number is less than 100");
            System.out.println("Hello");
        }
        if (n>100)
            System.out.println("the number is greater than 100");
    }
}
