
//a greater than b
//a less than b
//simple if sample
public class Sample13 {
    public static void main(String[] args) {
        int a=10;
        int b=10;
        if (a>b){
            System.out.println("a is greater than b");
        }
        if (a<b){
            System.out.println("a is less than b");
        }
        if (a==b){
            System.out.println("a is equal to b");
        }
        
    }
}
